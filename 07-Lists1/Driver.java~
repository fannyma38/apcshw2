  public class Driver{
      public static void main(String[] args){
  	WordSearch ws = new WordSearch();
  	ws.addWordH("up","morning",4,3);
  	ws.addWordV("southwest", "please",12,2);
  	ws.addWordD("down","yodada",1,2);
  	ws.addWordD("up","sausage",17,14);
  	ws.addWordH("down","ham",5,22);
  	ws.addWordV("down","gurgle",8,21);
  	ws.addWordD("southwest","death",4,9);
  	System.out.println(ws);
      }
  }
